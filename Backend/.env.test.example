# Test Environment Configuration
# Copy this file to .env.test and fill in your test database credentials

# Test Database URL (separate from development database)
# This should point to a dedicated test database that can be safely reset
DATABASE_URL_TEST=postgresql://postgres:password@localhost:5432/devjournal_test

# Direct URL for test database (for migrations)
DIRECT_URL_TEST=postgresql://postgres:password@localhost:5432/devjournal_test

# Supabase Test Configuration (optional - can use same as dev or mock)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
JWT_SECRET=your_jwt_secret_for_testing

# Gemini API (optional for testing - can be mocked)
GEMINI_API_KEY=your_gemini_api_key
